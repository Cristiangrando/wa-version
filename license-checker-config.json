{
  "ignore": [
    "**/.*",
    "**/*.md",
    "**/*.html",
    ".github",
    ".husky",
    "api-docs",
    "tokens"
  ],
  "license": "LICENSE.header",
  "licenseFormats": {
    "js|ts": {
      "prepend": "/*!",
      "append": " */\n// license end",
      "eachLine": {
        "prepend": " * "
      }
    },
    "dotfile": {
      "eachLine": {
        "prepend": "# "
      }
    }
  },
  "trailingWhitespace": "TRIM"
}
